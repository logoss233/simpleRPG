[application]

config/name="simple rpg"
run/main_scene="res://Main.tscn"
config/icon="res://icon.png"

[autoload]

Data="*res://autoload/Data.gd"
Game="*res://autoload/Game.gd"
TriggerSystem="*res://autoload/TriggerSystem.gd"
DmgObjFactory="*res://autoload/DmgObjFactory.gd"
Global="*res://autoload/Global.gd"
HealObjFactory="*res://autoload/HealObjFactory.gd"
BattleProcess="*res://autoload/BattleProcess.gd"

[debug]

gdscript/warnings/enable=false

[display]

window/size/width=1280
window/size/height=720
window/stretch/mode="2d"
window/stretch/aspect="keep"

[rendering]

quality/driver/driver_name="GLES2"
environment/default_environment="res://default_env.tres"
